<resources>

    <!-- Declare custom theme attributes that allow changing which styles are
         used for button bars depending on the API level.
         ?android:attr/buttonBarStyle is new as of API 11 so this is
         necessary to support previous API levels. -->
    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="metaButtonBarStyle" format="reference" />
        <attr name="metaButtonBarButtonStyle" format="reference" />
    </declare-styleable>
    <color name="arc_progress_color_df_1_0">#16f180</color>
    <color name="arc_progress_color_df_1_3">#0397ff</color>
    <color name="arc_progress_color_df_1_4">#0051ff</color>
    <color name="arc_progress_color_df_1_1">#27a8ff</color>
    <color name="arc_progress_color_df_1_2">#ffdc09</color>
    <color name="arc_progress_color_df_3">#fad961</color>
    <color name="arc_progress_color_df_5">#ff2600</color>
    <color name="arc_progress_color_df_6">#ff1500</color>
    <integer-array name="progress_colors_memory">
        <item>@color/arc_progress_color_df_1_1</item>
        <item>@color/arc_progress_color_df_1_2</item>
        <item>@color/arc_progress_color_df_5</item>
    </integer-array>

</resources>
